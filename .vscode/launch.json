  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug simple example",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/examples/simple/AppDiscord.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register/transpile-only"],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug multiple discord instances example",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/examples/multipleDiscordInstances/Main.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register/transpile-only"],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug commandsDir",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/examples/commandsDir/Main.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register/transpile-only"],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
